Sm from \"../../components/widgetSm/WidgetSm\";\r\nimport WidgetLg from \"../../components/widgetLg/WidgetLg\";\r\nimport { useEffect, useMemo, useState } from \"react\";\r\nimport { userRequest } from \"../../requestMethods\";\r\n\r\nexport default function Home() {\r\n  const [userStats, setUserStats] = useState([]);\r\n\r\n  const MONTHS = useMemo(\r\n    () => [\r\n      \"Jan\",\r\n      \"Feb\",\r\n      \"Mar\",\r\n      \"Apr\",\r\n      \"May\",\r\n      \"Jun\",\r\n      \"Jul\",\r\n      \"Agu\",\r\n      \"Sep\",\r\n      \"Oct\",\r\n      \"Nov\",\r\n      \"Dec\",\r\n    ],\r\n    []\r\n  );\r\n\r\n  useEffect(() => {\r\n    const getStats = async () => {\r\n      try {\r\n        const res = await userRequest.get(\"/users/stats\");\r\n        res.data.map((item) =>\r\n          setUserStats((prev) => [\r\n            ...prev,\r\n            { name: MONTHS[item._id - 1], \"Active User\": item.total },\r\n          ])\r\n        );\r\n      } catch {}\r\n    };\r\n    getStats();\r\n  }, [MONTHS]);\r\n\r\n  return (\r\n    <div className=\"home\">\r\n      <FeaturedInfo />\r\n      <Chart\r\n        data={userStats}\r\n        title=\"User Analytics\"\r\n        grid\r\n        dataKey=\"Active User\"\r\n      />\r\n      <div className=\"homeWidgets\">\r\n        <WidgetSm />\r\n        <WidgetLg />\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","D:\\you\\admin\\src\\pages\\userList\\UserList.jsx",[],"D:\\you\\admin\\src\\pages\\user\\User.jsx",[],"D:\\you\\admin\\src\\pages\\newUser\\NewUser.jsx",[],"D:\\you\\admin\\src\\pages\\productList\\ProductList.jsx",[],"D:\\you\\admin\\src\\pages\\product\\Product.jsx",["202"],"import { Link, useLocation } from \"react-router-dom\";\r\nimport \"./product.css\";\r\nimport Chart from \"../../components/chart/Chart\";\r\nimport { productData } from \"../../dummyData\";\r\nimport { Publish } from \"@material-ui/icons\";\r\nimport { useSelector } from \"react-redux\";\r\nimport { useEffect, useMemo, useState } from \"react\";\r\nimport { userRequest } from \"../../requestMethods\";\r\n\r\nexport default function Product() {\r\n  const location = useLocation();\r\n  const productId = location.pathname.split(\"/\")[2];\r\n  const [pStats, setPStats] = useState([]);\r\n\r\n  const product = useSelector((state) =>\r\n    state.product.products.find((product) => product._id === productId)\r\n  );\r\n\r\n  const MONTHS = useMemo(\r\n    () => [\r\n      \"Jan\",\r\n      \"Feb\",\r\n      \"Mar\",\r\n      \"Apr\",\r\n      \"May\",\r\n      \"Jun\",\r\n      \"Jul\",\r\n      \"Agu\",\r\n      \"Sep\",\r\n      \"Oct\",\r\n      \"Nov\",\r\n      \"Dec\",\r\n    ],\r\n    []\r\n  );\r\n\r\n  useEffect(() => {\r\n    const getStats = async () => {\r\n      try {\r\n        const res = await userRequest.get(\"orders/income?pid=\" + productId);\r\n        const list = res.data.sort((a,b)=>{\r\n            return a._id - b._id\r\n        })\r\n        list.map((item) =>\r\n          setPStats((prev) => [\r\n            ...prev,\r\n            { name: MONTHS[item._id - 1], Sales: item.total },\r\n          ])\r\n        );\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    };\r\n    getStats();\r\n  }, [productId, MONTHS]);\r\n\r\n  return (\r\n    <div className=\"product\">\r\n      <div className=\"productTitleContainer\">\r\n        <h1 className=\"productTitle\">Product</h1>\r\n        <Link to=\"/newproduct\">\r\n          <button className=\"productAddButton\">Create</button>\r\n        </Link>\r\n      </div>\r\n      <div className=\"productTop\">\r\n        <div className=\"productTopLeft\">\r\n          <Chart data={pStats} dataKey=\"Sales\" title=\"Sales Performance\" />\r\n        </div>\r\n        <div className=\"productTopRight\">\r\n          <div className=\"productInfoTop\">\r\n            <img src={product.img} alt=\"\" className=\"productInfoImg\" />\r\n            <span className=\"productName\">{product.title}</span>\r\n          </div>\r\n 